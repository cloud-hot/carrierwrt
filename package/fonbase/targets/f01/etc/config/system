config system
	option hostname Fonera
	option timezone Asia/Shanghai
	option timezone CST-8

config fon fon
	option firmware 2.3.8
	option device	fonera01
	option revision 0
	option beta "0+ (svn)"

config led usb
	option sysfs	gpio3
	option default	0

config led powerg
	option sysfs	gpio4
	option default	0

config led powero
	option sysfs	gpio7
	option default	1	

config led wifio
	option sysfs	gpio1
	option default	0

config button
	option button	reset
	option action	released
	option handler	"/etc/fonstated/Reset"
	option min		0
	option max		5

config button
	option button	reset
	option action	released
	option handler	"/etc/fonstated/ResetFactory"
	option min		6
	option max		100
