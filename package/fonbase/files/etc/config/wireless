
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT40+'
	option noscan '1'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option regdomain '0x35'
	option channel '6'
	option txpower '17'
	option chanbw '20'
	option hwmode '11ng'
	option disabled '0'
	option country '276'
