
config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option htmode 'HT40+'
	option noscan '1'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option regdomain '0x35'
	option channel '6'
	option txpower '17'
	option chanbw '20'
	option hwmode '11ng'
	option disabled '0'
	option country '276'

config wifi-iface 'ap_0'
	option device 'radio0'
	option ifname 'wlan0'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option maxassoc '30'
	option ssid 'MyPlace'
	option key 'dragino-dragino'
	option disabled '0'
	option encryption 'psk2'

config wifi-iface 'ap_1'
	option device 'radio0'
	option ifname 'wlan1'
	option network 'lan'
	option mode 'ap'
	option hidden '0'
	option maxassoc '30'
	option ssid 'FREE_INTERNET'
	option key 'dragino-dragino'
	option disabled '0'
	option encryption 'psk2'

config wifi-iface 'sta_0'
	option device 'radio0'
	option mode 'sta'
	option network 'wan'
	option disabled '1'

