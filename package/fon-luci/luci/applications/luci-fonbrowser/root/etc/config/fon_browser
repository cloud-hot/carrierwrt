config 'module' 'youtube'
	option 'template' 'youtube/browser'
	option 'priority' '80'
	list 'filter' "video/x-msvideo"
	list 'filter' "video/3gpp"
	list 'filter' "video/quicktime"
	list 'filter' "video/mp4"
	list 'filter' "video/mpeg"
	list 'filter' "video/mpeg"
	list 'filter' "video/mpeg"
	list 'filter' "video/v-flv"
	list 'filter' "application/x-shockwave-flash"
	list 'filter' "video/x-matroska"
	list 'filter' "video/x-ms-wmv"
	
