	option 'template' 'flickr/browser'
	option 'priority' '80'
	list 'filter' "image/jpeg"
	list 'filter' "image/jpeg"
	list 'filter' "image/gif"
	list 'filter' "image/bmp"
	list 'filter' "application/photoshop"
	list 'filter' "image/tiff"
	list 'filter' "image/tiff"
	list 'filter' "image/png"

