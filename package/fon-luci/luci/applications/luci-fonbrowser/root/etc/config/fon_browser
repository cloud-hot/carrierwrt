config 'general' 'browser'
	list 'path' '/tmp/mounts'
	list 'path' '/tmp/run/mountd'

config 'general' 'mime'
	option 'torrent' 'application/x-bittorrent'
	option 'mpeg' 'video/mpeg'
	option 'mpg' 'video/mpeg'
	option 'mpeg' 'video/mpeg'
	option 'mov' 'video/quicktime'
	option 'mp4' 'video/mp4'
	option jpeg 'image/jpeg'
	option jpg 'image/jpeg'
	option gif 'image/gif'
	option png 'image/png'
	option avi "video/x-msvideo"
	option 3gp "video/3gpp"
	option flv "video/v-flv"
	option swf "application/x-shockwave-flash"
	option mkv "video/x-matroska"
	option wmv "video/x-ms-wmv"
	option bmp "image/bmp"
	option psd "application/photoshop"
	option tif "image/tiff"
	option tiff "image/tiff"

